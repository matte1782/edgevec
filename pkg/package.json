    "url": "https://github.com/matte1782/edgevec"
  },
  "files": [
    "edgevec_bg.wasm",
    "edgevec.js",
    "edgevec.d.ts",
    "edgevec_bg.wasm.d.ts",
    "snippets",
    "index.ts",
    "filter.ts",
    "filter-builder.ts",
    "edgevec-wrapper.ts",
    "edgevec-types.d.ts",
    "LICENSE-APACHE",
    "LICENSE-MIT"
  ],
