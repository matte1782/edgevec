{
  "name": "edgevec",
  "type": "module",
  "description": "WASM-native vector database with SQL-like filtering, persistence, and soft delete",
  "version": "0.5.1",
  "license": "MIT OR Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/matte1782/edgevec"
  },
  "files": [
    "edgevec_bg.wasm",
    "edgevec.js",
    "edgevec.d.ts",
    "edgevec_bg.wasm.d.ts",
    "snippets",
    "index.ts",
    "filter.ts",
    "filter-builder.ts",
    "edgevec-wrapper.ts",
    "edgevec-types.d.ts",
    "LICENSE-APACHE",
    "LICENSE-MIT"
  ],
  "main": "edgevec.js",
  "types": "edgevec.d.ts",
  "exports": {
    ".": {
      "types": "./edgevec-types.d.ts",
      "import": "./index.ts"
    },
    "./filter": {
      "types": "./filter.ts",
      "import": "./filter.ts"
    },
    "./filter-builder": {
      "types": "./filter-builder.ts",
      "import": "./filter-builder.ts"
    },
    "./wrapper": {
      "types": "./edgevec-wrapper.ts",
      "import": "./edgevec-wrapper.ts"
    }
  },
  "sideEffects": [
    "./snippets/*"
  ],
  "keywords": [
    "vector-database",
    "vector-search",
    "hnsw",
    "wasm",
    "webassembly",
    "filtered-search",
    "metadata-filtering",
    "similarity-search",
    "embeddings",
    "semantic-search",
    "browser",
    "edge-computing",
    "offline-first",
    "indexeddb",
    "soft-delete",
    "persistence"
